<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".settingsFragment">

    <ImageView
        android:id="@+id/imageView84"
        android:layout_width="372dp"
        android:layout_height="400dp"
        android:src="@drawable/rectangle_shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView85"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/imageView84"
        app:layout_constraintTop_toTopOf="@+id/imageView84"
        app:srcCompat="@drawable/user_icon" />

    <TextView
        android:id="@+id/tvConfigNotifUsername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/outfit_medium"
        android:textColor="@color/primaryText"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView84"
        app:layout_constraintStart_toStartOf="@+id/imageView84"
        app:layout_constraintTop_toTopOf="@+id/imageView84" />

    <ImageView
        android:id="@+id/imageView86"
        android:layout_width="372sp"
        android:layout_height="2sp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView84"
        app:layout_constraintStart_toStartOf="@+id/imageView84"
        app:layout_constraintTop_toBottomOf="@+id/tvConfigNotifUsername"
        app:srcCompat="@drawable/line_divider_pink" />

    <TextView
        android:id="@+id/textView158"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/outfit_medium"
        android:text="Account Settings"
        android:textColor="@color/tertiaryDarkText"
        app:layout_constraintStart_toStartOf="@+id/imageView84"
        app:layout_constraintTop_toBottomOf="@+id/imageView86" />

    <TextView
        android:id="@+id/textView159"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/outfit_medium"
        android:text="Edit Profile"
        android:textColor="@color/primaryText"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/imageView84"
        app:layout_constraintTop_toBottomOf="@+id/textView158" />

    <TextView
        android:id="@+id/textView160"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/outfit_medium"
        android:text="Change Password"
        android:textColor="@color/primaryText"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/imageView84"
        app:layout_constraintTop_toBottomOf="@+id/textView159" />

    <TextView
        android:id="@+id/textView161"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/outfit_medium"
        android:text="Notifications"
        android:textColor="@color/primaryText"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/imageView84"
        app:layout_constraintTop_toBottomOf="@+id/textView160" />

    <ImageView
        android:id="@+id/imConfigNotifEditProfile"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView84"
        app:layout_constraintTop_toBottomOf="@+id/textView158"
        app:srcCompat="@drawable/arrow_right_black" />

    <ImageView
        android:id="@+id/imConfigNotifChangePass"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView84"
        app:layout_constraintTop_toBottomOf="@+id/imConfigNotifEditProfile"
        app:srcCompat="@drawable/arrow_right_black" />

    <ImageView
        android:id="@+id/imConfigNotif"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView84"
        app:layout_constraintTop_toBottomOf="@+id/imConfigNotifChangePass"
        app:srcCompat="@drawable/arrow_down_icon" />

    <ImageView
        android:id="@+id/imageView109"
        android:layout_width="372dp"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView84"
        app:layout_constraintStart_toStartOf="@+id/imageView84"
        app:layout_constraintTop_toBottomOf="@+id/tvConfigNotif"
        app:srcCompat="@drawable/line_divider" />

    <TextView
        android:id="@+id/textView162"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/outfit_medium"
        android:text="More"
        android:textColor="@color/tertiaryDarkText"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/imageView109"
        app:layout_constraintTop_toBottomOf="@+id/imageView109" />

    <TextView
        android:id="@+id/textView185"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/outfit_medium"
        android:text="Design"
        android:textColor="@color/primaryText"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/imageView109"
        app:layout_constraintTop_toBottomOf="@+id/textView162" />

    <ImageView
        android:id="@+id/imConfigNotifDesign"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView109"
        app:layout_constraintTop_toBottomOf="@+id/textView162"
        app:srcCompat="@drawable/arrow_right_black" />

    <TextView
        android:id="@+id/tvConfigNotif"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/outfit_medium"
        android:text="Turn On Notification"
        android:textColor="@color/primaryText"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/imageView84"
        app:layout_constraintTop_toBottomOf="@+id/textView161" />

    <Switch
        android:id="@+id/sConfigNotif"
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView84"
        app:layout_constraintTop_toBottomOf="@+id/imConfigNotif" />
</androidx.constraintlayout.widget.ConstraintLayout>