<resources  xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppDefaultTheme" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primaryDark</item>
    </style>
    <style name="AppOrangeTheme" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="colorPrimary">@color/orange</item>
        <item name="colorPrimaryDark">@color/orangeDark</item>
    </style>
    <style name="AppGreenTheme" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="colorPrimary">@color/green</item>
        <item name="colorPrimaryDark">@color/greenDark</item>
    </style>

    <style name="BottomNavTextActiveStyle" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/whiteText</item>
        <item name="android:fontFamily">@font/outfit_semi_bold</item>
    </style>

    <style name="BottomNavTextInactiveStyle" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/tertiaryText</item>
        <item name="android:fontFamily">@font/outfit_semi_bold</item>
    </style>

</resources>